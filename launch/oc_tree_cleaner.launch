<launch>
  <include file="$(find pr2_moveit_config)/launch/planning_context.launch" />

  <node pkg="kinematics_cache" type="oc_tree_cleaner" name="left_arm_oc_tree_cleaner" output="screen">
    <param name="arm" value="left_arm" />
    <param name="other_arm" value="right_arm" />

    <param name="input" value="$(find kinematics_cache)/octree/left_arm.ot" />
    <param name="output" value="/home/jlurz/catkin_ws/src/kinematics_cache/octree/left_arm.cleaned.ot" />
  </node>

  <node pkg="kinematics_cache" type="oc_tree_cleaner" name="right_arm_oc_tree_cleaner" output="screen">
    <param name="arm" value="right_arm" />
    <param name="other_arm" value="left_arm" />

    <param name="input" value="$(find kinematics_cache)/octree/right_arm.ot" />
    <param name="output" value="/home/jlurz/catkin_ws/src/kinematics_cache/octree/right_arm.cleaned.ot" />
  </node>
</launch>
